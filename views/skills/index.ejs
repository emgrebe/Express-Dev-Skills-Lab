<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Express Dev-Skills Lab</title>
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <% skills.forEach(function(s, idx) { %>
      <li>
        <a href="/skills/<%= idx %>"><%= s.skill %></a>
        <%= s.skill %>
          - 
        <%= s.done ? 'done' : 'not done' %>
      </li>
    <% }); %>
  </ul>
</body>
</html>